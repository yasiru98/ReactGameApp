<html lang="en">
<head>
  <meta charset="utf-8"/>
  <title>CyberGames</title>
  <script src="https://unpkg.com/react@latest/umd/react.development.js" crossorigin="anonymous"></script>
  <script src="https://unpkg.com/react-dom@latest/umd/react-dom.development.js"></script>
  <script src="https://unpkg.com/@material-ui/core@latest/umd/material-ui.development.js" crossorigin="anonymous"></script>
  <script src="https://unpkg.com/babel-standalone@latest/babel.min.js" crossorigin="anonymous"></script>
  <script crossorigin src="https://unpkg.com/prop-types/prop-types.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/remarkable/1.6.2/remarkable.min.js"></script>
  <!-- "https://icons8.com/icons/set/controller">Game Controller favicon icon -->
  <link rel="shortcut icon" href="/assets/img/favicon.png"/>
  <link rel="stylesheet" type="text/css" href="/assets/style.css"/>
    <style>
    * {
      box-sizing: border-box;
    }

    .row {
      display: flex;
      height: 100px;
    }

    /* Create three equal columns that sits next to each other */
    .column {
      flex: 33.33%;
      padding: 5px;
    }
  </style>
</head>
<body> 

  <!-- "https://icons8.com/icons/set/controller">Game Controller Nav bar icon -->
  <nav><a href="/choose"><img id="logo" src="/assets/img/controller.png" alt="controller logo" /></a>
    <div class="navlink"><a href="/choose"> Games</a></div>
    <div class="navlink"><a href="/logout">Log out</a></div>
  </nav>

  <div id="pageHeading"></div>

 <div id="root">

</div>
  <div class="row">
    <div class="column">
      <img src="/assets/img/img_snow.jpg" alt="Snow" style="width:100%;; height:200px">
    </div>
    <div class="column">
      <img src="/assets/img/img_forest.jpg" alt="Forest" style="width:100%;; height:200px">
    </div>
    <div class="column">
      <img src="/assets/img/img_mountains.jpg" alt="Mountains" style="width:100%; height:200px">
    </div>
  </div>
    <!-- "http://clipart-library.com/clip-art/1336561_monster-cartoon-png.htm">Monster Icon -->
  <div id="monsterMessage"><img id="speech" src="/assets/img/speech.png" alt="speech box" />
    <h3><span id="errorMessage"></span></h3><img id="monster" src="assets/img/monster.png" alt="monster" />
  </div>
 <script src="/assets/passbundle.js"></script>
</body>
</html>